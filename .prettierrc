{
  "useTabs": false,
  "singleQuote": true,
  "bracketSameLine": true,
  "trailingComma": "none",
  "printWidth": 95,
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
